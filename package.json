{
  "name": "web-proj-assignment-2",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc && node --env-file=.env dist/app.js",
    "dev": "nodemon --exec ts-node -r dotenv/config src/app.ts",
    "debug": "node --env-file=.env --inspect dist/app.js",
    "coverage": "tsc && jest --detectOpenHandles --coverage --runInBand --forceExit",
    "test": "tsc && jest --detectOpenHandles --runInBand --forceExit",
    "usersTest": "tsc && jest --detectOpenHandles --forceExit --runInBand ./src/tests/users.test.ts",
    "commentsTest": "tsc && jest --detectOpenHandles --forceExit --runInBand ./src/tests/comments.test.ts",
    "postsTest": "tsc && jest --detectOpenHandles --forceExit --runInBand ./src/tests/posts.test.ts",
    "authTest": "tsc && jest --detectOpenHandles --forceExit --runInBand ./src/tests/auth.test.ts"
  },
  "author": "Amit Sahar And Sivan Alchasov",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "@types/express": "^4.17.21"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/supertest": "^2.0.16",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}